AAA,IAAA,EAAA;AACA,QAAA,IAAA,CAAA,IAAA,GAAA,IAAA;AACA,OAFA;AAGA,KA3BA;AA4BA;AACA,IAAA,YA7BA,0BA6BA;AACA,WAAA,KAAA,GAAA,KAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAFA,CAAA;AAGA;AAjCA,GAdA;AAiDA,EAAA,KAAA,EAAA;AACA,IAAA,KAAA,EAAA;AACA,MAAA,IAAA,EAAA,IADA;AAEA,MAAA,OAFA,mBAEA,KAFA,EAEA;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,CAAA;AACA;AAJA;AADA,GAjDA;AAyDA,EAAA,OAzDA,qBAyDA;AAAA;;AACA,SAAA,IAAA,CAAA,GAAA,CAAA,WAAA,EAAA,KAAA,SAAA;AACA,SAAA,IAAA,CAAA,GAAA,CAAA,YAAA,EAAA,KAAA,UAAA;AACA,SAAA,KAAA,GAAA,MAAA,CAAA,SAAA,CAAA,YAAA,EAAA,KAAA,UAAA,CAAA;AACA;;AACA,SAAA,KAAA,GAAA,WAAA,CAAA,YAAA;AACA,MAAA,KAAA,CAAA,QAAA,GAAA,KAAA,CAAA,KAAA,GAAA,MAAA,CAAA,qBAAA,CAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA;AACA,KAHA,EAGA,IAHA,CAAA;AAIA,GAlEA;AAmEA,EAAA,aAnEA,2BAmEA;AACA,SAAA,IAAA,CAAA,IAAA,CAAA,WAAA;AACA,SAAA,IAAA,CAAA,IAAA,CAAA,YAAA;AACA,IAAA,MAAA,CAAA,WAAA,CAAA,KAAA,KAAA;;AACA,QAAA,KAAA,KAAA,EAAA;AACA,MAAA,aAAA,CAAA,KAAA,KAAA,CAAA;AACA;AACA;AA1EA,CAAA","sourcesContent":["<template>\r\n  <div id=\"root\">\r\n    <h2 class=\"come\">现在是北京时间：{{this.datetime}}</h2> \r\n    <div class=\"todo-container\">\r\n      <div class=\"todo-wrap\">\r\n        <MyHeader @addTodo=\"addTodo\" />\r\n        <MyList :todos=\"todos\" />\r\n        <MyFooter\r\n          :todos=\"todos\"\r\n          @checkAllTodo=\"checkAllTodo\"\r\n          @clearAllTodo=\"clearAllTodo\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n\r\n\r\nimport pubsub from \"pubsub-js\";\r\nimport MyHeader from \"./components/MyHeader\";\r\nimport MyList from \"./components/MyList\";\r\nimport MyFooter from \"./components/MyFooter\";\r\n\r\nexport default {\r\n  name: \"App\",\r\n  components: {\r\n    MyHeader,\r\n    MyList,\r\n    MyFooter,\r\n  },\r\n  data() {\r\n    return {\r\n      todos: JSON.parse(localStorage.getItem(\"todos\")) || [],\r\n       timer: '',\r\n        datetime: ''\r\n    };\r\n  },\r\n  methods: {\r\n    //添加一个todo\r\n    addTodo(todoObj) {\r\n      this.todos.unshift(todoObj);\r\n    },\r\n    //勾选or取消勾选一个todo\r\n    checkTodo(id) {\r\n      this.todos.forEach((todo) => {\r\n        if (todo.id === id) todo.done = !todo.done;\r\n      });\r\n    },\r\n\t//更新一个todo\r\n    updateTodo(id, title) {\r\n      this.todos.forEach((todo) => {\r\n        if (todo.id === id) todo.title = title;\r\n      });\r\n    },\r\n    //删除一个todo\r\n    deleteTodo(_, id) {\r\n      this.todos = this.todos.filter((todo) => todo.id !== id);\r\n    },\r\n    \r\n    //全选or取消全选\r\n    checkAllTodo(done) {\r\n      this.todos.forEach((todo) => {\r\n        todo.done = done;\r\n      });\r\n    },\r\n    //清除所有已经完成的todo\r\n    clearAllTodo() {\r\n      this.todos = this.todos.filter((todo) => {\r\n        return !todo.done;\r\n      });\r\n    },\r\n  },\r\n  watch: {\r\n    todos: {\r\n      deep: true,\r\n      handler(value) {\r\n        localStorage.setItem(\"todos\", JSON.stringify(value));\r\n      },\r\n    },\r\n  },\r\n  mounted() {\r\n    this.$bus.$on(\"checkTodo\", this.checkTodo);\r\n    this.$bus.$on(\"updateTodo\", this.updateTodo);\r\n    this.pubId = pubsub.subscribe(\"deleteTodo\", this.deleteTodo);\r\n     /* 每秒定时刷新 */\r\n      this.timer = setInterval(() => {\r\n        this.datetime = this.dayjs().format(\"YYYY-MM-DD HH:mm:ss\")\r\n        console.log(this.datetime)\r\n      }, 1000)\r\n  },\r\n  beforeDestroy() {\r\n    this.$bus.$off(\"checkTodo\");\r\n\tthis.$bus.$off(\"updateTodo\");\r\n    pubsub.unsubscribe(this.pubId);\r\n     if(this.timer){\r\n        clearInterval(this.timer);\r\n      }\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/*base*/\r\nbody {\r\n  background: #fff;\r\n}\r\nh2{\r\n  text-align: center;\r\n  color: skyblue;\r\n}\r\n.come{\r\n    animation: donhua 1s;\r\n}\r\n@keyframes donhua {\r\n    from{\r\n        transform: translateX(-100%);\r\n    }\r\n}\r\n.btn {\r\n  display: inline-block;\r\n  padding: 4px 12px;\r\n  margin-bottom: 0;\r\n  font-size: 14px;\r\n  line-height: 20px;\r\n  text-align: center;\r\n  vertical-align: middle;\r\n  cursor: pointer;\r\n  box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2),\r\n    0 1px 2px rgba(0, 0, 0, 0.05);\r\n  border-radius: 4px;\r\n}\r\n.btn-danger {\r\n  color: #fff;\r\n  background-color: #da4f49;\r\n  border: 1px solid #bd362f;\r\n}\r\n.btn-danger:hover {\r\n  color: #fff;\r\n  background-color: #bd362f;\r\n}\r\n.btn-edit {\r\n  color: #fff;\r\n  background-color: skyblue;\r\n  border: 1px solid rgb(103, 159, 180);\r\n  margin-right: 5px;\r\n}\r\n.btn:focus {\r\n  outline: none;\r\n}\r\n.todo-container {\r\n  width: 600px;\r\n  margin: 0 auto;\r\n}\r\n.todo-container .todo-wrap {\r\n  padding: 10px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 5px;\r\n}\r\n</style>\r\n\r\n"],"sourceRoot":"src"}]}