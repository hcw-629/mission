ACA,OAFA;AAGA,KAhBA;AAiBA;AACA,IAAA,UAlBA,sBAkBA,CAlBA,EAkBA,EAlBA,EAkBA;AACA,WAAA,KAAA,GAAA,KAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,EAAA,KAAA,EAAA;AAAA,OAAA,CAAA;AACA,KApBA;AAsBA;AACA,IAAA,YAvBA,wBAuBA,IAvBA,EAuBA;AACA,WAAA,KAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,QAAA,IAAA,CAAA,IAAA,GAAA,IAAA;AACA,OAFA;AAGA,KA3BA;AA4BA;AACA,IAAA,YA7BA,0BA6BA;AACA,WAAA,KAAA,GAAA,KAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAFA,CAAA;AAGA;AAjCA,GAbA;AAgDA,EAAA,KAAA,EAAA;AACA,IAAA,KAAA,EAAA;AACA,MAAA,IAAA,EAAA,IADA;AAEA,MAAA,OAFA,mBAEA,KAFA,EAEA;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,CAAA;AACA;AAJA;AADA,GAhDA;AAwDA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,UAAA,EAAA,oBAAA,KAAA,EAAA;AACA;AACA,UAAA,IAAA,GAAA,IAAA,IAAA,EAAA;AACA,UAAA,IAAA,GAAA,IAAA,CAAA,WAAA,EAAA;AACA,UAAA,KAAA,GAAA,OAAA,CAAA,IAAA,CAAA,QAAA,KAAA,CAAA,CAAA;AACA,UAAA,GAAA,GAAA,OAAA,CAAA,IAAA,CAAA,QAAA,EAAA,CAAA;AACA,UAAA,KAAA,GAAA,OAAA,CAAA,IAAA,CAAA,QAAA,EAAA,CAAA;AACA,UAAA,OAAA,GAAA,OAAA,CAAA,IAAA,CAAA,UAAA,EAAA,CAAA;AACA,UAAA,OAAA,GAAA,OAAA,CAAA,IAAA,CAAA,UAAA,EAAA,CAAA,CARA,CASA;;AACA,aACA,IAAA,GACA,GADA,GAEA,KAFA,GAGA,GAHA,GAIA,GAJA,GAKA,GALA,GAMA,KANA,GAOA,GAPA,GAQA,OARA,GASA,GATA,GAUA,OAXA;AAaA;AAzBA,GAxDA;AAoFA,EAAA,OApFA,qBAoFA;AACA,SAAA,IAAA,CAAA,GAAA,CAAA,WAAA,EAAA,KAAA,SAAA;AACA,SAAA,IAAA,CAAA,GAAA,CAAA,YAAA,EAAA,KAAA,UAAA;AACA,SAAA,KAAA,GAAA,MAAA,CAAA,SAAA,CAAA,YAAA,EAAA,KAAA,UAAA,CAAA;;AACA,QAAA,KAAA,GAAA,IAAA,CAJA,CAIA;;;AACA,SAAA,KAAA,GAAA,WAAA,CAAA,YAAA;AACA,MAAA,KAAA,CAAA,IAAA,GAAA,IAAA,IAAA,EAAA,CADA,CACA;AACA,KAFA,EAEA,IAFA,CAAA;AAGA,GA5FA;AA6FA,EAAA,aA7FA,2BA6FA;AACA,SAAA,IAAA,CAAA,IAAA,CAAA,WAAA;AACA,SAAA,IAAA,CAAA,IAAA,CAAA,YAAA;AACA,IAAA,MAAA,CAAA,WAAA,CAAA,KAAA,KAAA;;AACA,QAAA,KAAA,KAAA,EAAA;AACA,MAAA,aAAA,CAAA,KAAA,KAAA,CAAA,CADA,CACA;AACA;AACA;AApGA,CAAA","sourcesContent":["<template>\r\n  <div id=\"root\">\r\n    <!-- 北京时间 -->\r\n    <!-- 过滤器实现（传参） -->\r\n    <h3>现在是北京时间：{{ date | formatDate }}</h3>\r\n    <div class=\"todo-container\">\r\n      <div class=\"todo-wrap\">\r\n        <MyHeader @addTodo=\"addTodo\" />\r\n        <MyList :todos=\"todos\" />\r\n        <MyFooter\r\n          :todos=\"todos\"\r\n          @checkAllTodo=\"checkAllTodo\"\r\n          @clearAllTodo=\"clearAllTodo\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport pubsub from \"pubsub-js\";\r\nimport MyHeader from \"./components/MyHeader\";\r\nimport MyList from \"./components/MyList\";\r\nimport MyFooter from \"./components/MyFooter\";\r\nvar padDate = function (value) {    //在月份、日期、小时等小于10时在前面补0\r\n          return value<10?'0'+value:value;\r\n        };\r\nexport default {\r\n  name: \"App\",\r\n  components: {\r\n    MyHeader,\r\n    MyList,\r\n    MyFooter,\r\n  },\r\n  data() {\r\n    return {\r\n      todos: JSON.parse(localStorage.getItem(\"todos\")) || [],\r\n      date: new Date(),\r\n    };\r\n  },\r\n  methods: {\r\n    //添加一个todo\r\n    addTodo(todoObj) {\r\n      this.todos.unshift(todoObj);\r\n    },\r\n    //勾选or取消勾选一个todo\r\n    checkTodo(id) {\r\n      this.todos.forEach((todo) => {\r\n        if (todo.id === id) todo.done = !todo.done;\r\n      });\r\n    },\r\n    //更新一个todo\r\n    updateTodo(id, title) {\r\n      this.todos.forEach((todo) => {\r\n        if (todo.id === id) todo.title = title;\r\n      });\r\n    },\r\n    //删除一个todo\r\n    deleteTodo(_, id) {\r\n      this.todos = this.todos.filter((todo) => todo.id !== id);\r\n    },\r\n\r\n    //全选or取消全选\r\n    checkAllTodo(done) {\r\n      this.todos.forEach((todo) => {\r\n        todo.done = done;\r\n      });\r\n    },\r\n    //清除所有已经完成的todo\r\n    clearAllTodo() {\r\n      this.todos = this.todos.filter((todo) => {\r\n        return !todo.done;\r\n      });\r\n    },\r\n  },\r\n  watch: {\r\n    todos: {\r\n      deep: true,\r\n      handler(value) {\r\n        localStorage.setItem(\"todos\", JSON.stringify(value));\r\n      },\r\n    },\r\n  },\r\n  filters: {\r\n    //过滤器\r\n    formatDate: function (value) {\r\n      //value为需要过滤的数据\r\n      var date = new Date();\r\n      var year = date.getFullYear();\r\n      var month = padDate(date.getMonth() + 1);\r\n      var day = padDate(date.getHours());\r\n      var hours = padDate(date.getHours());\r\n      var minutes = padDate(date.getMinutes());\r\n      var seconds = padDate(date.getSeconds());\r\n      //整理数据并返回\r\n      return (\r\n        year +\r\n        \"-\" +\r\n        month +\r\n        \"-\" +\r\n        day +\r\n        \" \" +\r\n        hours +\r\n        \":\" +\r\n        minutes +\r\n        \":\" +\r\n        seconds\r\n      );\r\n    },\r\n  },\r\n\r\n  mounted() {\r\n    this.$bus.$on(\"checkTodo\", this.checkTodo);\r\n    this.$bus.$on(\"updateTodo\", this.updateTodo);\r\n    this.pubId = pubsub.subscribe(\"deleteTodo\", this.deleteTodo);\r\n    var _this = this; //声明一个变量指向Vue实例this，保证作用域一致\r\n    this.timer = setInterval(function () {\r\n      _this.date = new Date(); //修改数据date\r\n    }, 1000);\r\n  },\r\n  beforeDestroy() {\r\n    this.$bus.$off(\"checkTodo\");\r\n    this.$bus.$off(\"updateTodo\");\r\n    pubsub.unsubscribe(this.pubId);\r\n    if (this.timer) {\r\n      clearInterval(this.timer); //在Vue实例销毁前，清除定时器\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/*base*/\r\nbody {\r\n  background: #fff;\r\n}\r\n.btn {\r\n  display: inline-block;\r\n  padding: 4px 12px;\r\n  margin-bottom: 0;\r\n  font-size: 14px;\r\n  line-height: 20px;\r\n  text-align: center;\r\n  vertical-align: middle;\r\n  cursor: pointer;\r\n  box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2),\r\n    0 1px 2px rgba(0, 0, 0, 0.05);\r\n  border-radius: 4px;\r\n}\r\n.btn-danger {\r\n  color: #fff;\r\n  background-color: #da4f49;\r\n  border: 1px solid #bd362f;\r\n}\r\n.btn-danger:hover {\r\n  color: #fff;\r\n  background-color: #bd362f;\r\n}\r\n.btn-edit {\r\n  color: #fff;\r\n  background-color: skyblue;\r\n  border: 1px solid rgb(103, 159, 180);\r\n  margin-right: 5px;\r\n}\r\n.btn:focus {\r\n  outline: none;\r\n}\r\n.todo-container {\r\n  width: 600px;\r\n  margin: 0 auto;\r\n}\r\n.todo-container .todo-wrap {\r\n  padding: 10px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 5px;\r\n}\r\n</style>\r\n\r\n"],"sourceRoot":"src"}]}